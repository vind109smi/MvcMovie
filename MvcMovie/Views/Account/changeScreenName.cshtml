@model MvcMovie.Models.Accounts.ApplicationUser


@{
    ViewData["Title"] = "changeScreenName";
}

<h2>
    Change Screen Name
</h2>
<div class="row">
    <div class="col-md-4">
            <form asp-route-returnurl="@ViewData["ReturnUrl"]" method="post">
                <h4>Create a new account.</h4>
                <hr />
                <div asp-validation-summary="All" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="ScreenName"></label>
                    <input asp-for="ScreenName" class="form-control" />
                    <span asp-validation-for="ScreenName" class="text-danger"></span>
                </div>
                    <button type="submit" class="btn btn-default">Change Screen Name</button>  
            </form>
    </div>
</div>

